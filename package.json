{
  "name": "lynx",
  "version": "1.0.0",
  "private": true,
  "author": {
    "name": "ANDRiOS",
    "email": "elourenco@outlook.com"
  },
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test:unit": "jest",
    "test": "yarn lint && yarn flow && jest",
    "lint": "eslint src",
    "clear-cache": "rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-* && rm -rf node_modules/ && yarn cache clean && yarn install"
  },
  "lint-staged": {
    "*.js": [
      "git add"
    ]
  },
  "jest": {
    "preset": "react-native",
    "testMatch": [
      "**/?(*.)test.js?(x)"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "./jest/setup.js"
    ]
  },
  "prettier": {
    "jsxBracketSameLine": true,
    "semi": false,
    "singleQuote": true
  },
  "rnpm": {
    "assets": [
      "./assets/fonts"
    ]
  },
  "dependencies": {
    "prop-types": "^15.6.2",
    "react": "16.6.1",
    "react-native": "0.57.5",
    "react-native-custom-tabs": "^0.1.7",
    "react-native-safari-view": "^2.1.0",
    "react-native-vector-icons": "^6.0.2",
    "react-navigation": "~2.18.0",
    "react-redux": "~5.0.7",
    "redux": "~4.0.1",
    "redux-logger": "~3.0.6",
    "redux-thunk": "~2.3.0"
  },
  "devDependencies": {
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "23.6.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-preset-airbnb": "^3.0.1",
    "babel-preset-react-native": "^4.0.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-babel": "^5.2.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.25.1",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-native": "^3.5.0",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-watch": "^4.0.2",
    "husky": "^1.1.2",
    "jest": "23.6.0",
    "lint-staged": "^7.3.0",
    "metro-react-native-babel-preset": "0.49.2",
    "pre-push": "^0.1.1",
    "prettier": "^1.14.3",
    "react-devtools": "^3.4.1",
    "react-dom": "^16.5.2",
    "react-test-renderer": "16.6.1"
  }
}
